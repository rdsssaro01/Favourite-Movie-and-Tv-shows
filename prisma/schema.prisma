generator client {
  provider = "prisma-client"
  binaryTargets = ["native", "rhel-openssl-3.0.x"] 
  engineType    = "binary" 
  // output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Entry {
id String @id @default(cuid())
title String?  @unique 
type String
director String?
budget Int?
location String?
duration Int?
year Int?
createdAt DateTime @default(now())
updatedAt DateTime @updatedAt
}

